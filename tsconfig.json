{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "./dist",
      "types": ["vite/client"]
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
  }
  